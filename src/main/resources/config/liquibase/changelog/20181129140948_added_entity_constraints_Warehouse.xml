<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Warehouse.
    -->
    <changeSet id="20181129140948-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="warehouse"
                                 constraintName="fk_warehouse_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="warehouses_id"
                                 baseTableName="warehouse_image"
                                 constraintName="fk_warehouse_image_warehouses_id"
                                 referencedColumnNames="id"
                                 referencedTableName="warehouse"/>
        <addForeignKeyConstraint baseColumnNames="images_id"
                                 baseTableName="warehouse_image"
                                 constraintName="fk_warehouse_image_images_id"
                                 referencedColumnNames="id"
                                 referencedTableName="image"/>
        
    </changeSet>
</databaseChangeLog>
